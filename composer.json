{
  "name": "slick/webapp",
  "description": "Basic template for a web application built with the Slick framework.",
  "license": "MIT",
  "version": "v2.1.7",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\App\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "league/oauth2-client": "^2.7",
    "nyholm/psr7": "^1.8",
    "slick/error-handler": "^0.4.0",
    "slick/http": "^3.0",
    "slick/template": "^3.0@dev",
    "slick/webstack": "^2.0@dev",
    "vlucas/phpdotenv": "^5.6",
    "xeroapi/xero-php-oauth2": "^7.3"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4"
  }
}
